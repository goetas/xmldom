{
    "name": "goetas/xmldom",
    "type": "symfony-bundle",
    "description": "Goetas XML DOMDocument extension",
    "keywords": ["xml", "dom"],
    "homepage": "http://opensource.mercuriosistemi.com/projects/show/xmldom",
    "license": "MIT",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@lignano.it"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {
    	"psr-0": { "goetas\\xml": "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
		"phing/phing": "~2.5",
		"phpmd/phpmd": "~1.4",
		"squizlabs/php_codesniffer": "~1.4",
		"sebastian/phpcpd": "~1.4",
		"zerkalica/php-code-browser": "dev-master"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
